                  </form>
               </div>
               <!-- /.history-form__top -->
               <form action="#" class="head head__form">
                  <div class="head-block">
                     <div class="head-number">
                        <input type="text" class="input head-number__request" placeholder="Номер заявки">
                        <a href="#" class="head-number__button">
                           <img src="img/arrow3.svg" alt="ARROW" class="head-number__arrow3">
                        </a>
                     </div>
                     <div class="head-number head-number__period--block">
